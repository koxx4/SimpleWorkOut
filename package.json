{
  "name": "simpleworkout",
  "version": "1.0.0",
  "description": "Simple web app that allows user to log their outdoor workouts",
  "default": "src/html/index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss:compile": "sass --color --stop-on-error --no-error-css scss/style.scss css/style.css",
    "scss:start-server": "sass --watch --color --no-error-css scss/style.scss css/style.css",
    "build:dev": "parcel watch src/html/index.html --dist-dir ./dev-dist",
    "build:dev-start-server": "parcel src/html/index.html --dist-dir ./dev-dist --open",
    "build:dev-all": "npm-run-all scss:compile build:dev",
    "build:dev-start-all-servers": "npm-run-all -n -p scss:start-server build:dev-start-server",
    "build:production": "parcel build src/html/index.html --dist-dir ./dist",
    "build": "npm-run-all -n scss:compile build:production",
    "clean:dist": "rm -rf dist/*",
    "clean:dev-dist": "rm -rf dev-dist/*",
    "clean:compiled-css": "rm -rf css/*",
    "clean": "npm-run-all -p clean:dist clean:dev-dist clean:compiled-css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koxx4/SimpleWorkOut.git"
  },
  "keywords": [
    "workout",
    "netlify"
  ],
  "author": "Piotr Wilk",
  "license": "LGPL-2.1-or-later",
  "bugs": {
    "url": "https://github.com/koxx4/SimpleWorkOut/issues"
  },
  "homepage": "https://github.com/koxx4/SimpleWorkOut#readme",
  "dependencies": {
    "@popperjs/core": "^2.9.3",
    "bootstrap": "^5.1.0",
    "bootstrap-icons": "^1.5.0",
    "core-js": "^3.16.4",
    "leaflet": "^1.7.1",
    "lodash-es": "^4.17.21",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.38.2"
  },
  "devDependencies": {
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-rc.0",
    "prettier": "^2.3.2"
  }
}
